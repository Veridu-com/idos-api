{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://api.veridu.com/1.0/schema/attribute/attributeEntity.json",
    "title": "Attribute",
    "type": "object",
    "properties": {
        "name": {
            "description": "Attribute name",
            "type": "string",
            "pattern": ""
        },
        "value": {
            "description": "Attribute value",
            "type": "string",
            "pattern": ""
        },
        "created_at": {
            "description": "Attribute's creation unixtime",
            "type": "integer"
        },
        "updated_at": {
            "description": "Attribute's last updated unixtime",
            "type": ["integer", "null"]
        }
    },
    "required": [
        "name",
        "value",
        "created_at"
    ]
}
