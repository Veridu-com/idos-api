{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "GET /management/credentials/{pubKey}/hooks",
    "type": "object",
    "properties": {
        "status": {
            "description": "Response status flag",
            "type": "boolean"
        },
        "data": {
            "description": "Hook response payload",
            "type": "array",
            "items": {
                "$ref": "hookEntity.json"
            },
            "minItems": 0,
            "uniqueItems": true
        },
        "updated": {
            "description": "Unixtime of the last update",
            "type": "integer"
        }
    },
    "required": [
        "status",
        "data",
        "updated"
    ]
}

