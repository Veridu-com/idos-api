{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://api.idos.io/1.0/schema/user/gateEntity.json",
    "title": "Gate",
    "type": "object",
    "properties": {
        "id": {
            "description": "Gate id",
            "type": "integer"
        },
        "name": {
            "description": "Gate's name",
            "type": "string",
            "pattern": "[a-zA-Z0-9_-]+"
        },
        "slug": {
            "description": "Gate's slug",
            "type": "string",
            "pattern": "[a-zA-Z0-9_-]+"
        },
        "pass": {
            "description": "Gate value",
            "type": "boolean"
        },
        "created_at": {
            "description": "Gate's creation unixtime",
            "type": "integer"
        },
        "updated_at": {
            "description": "Gate's last update unixtime",
            "type": ["integer", "null"]
        }
    },
    "dependencies": {
        "slug": ["name", "confidence_level"]
    },
    "required": [
        "id",
        "name",
        "slug",
        "pass",
        "created_at"
    ]
}
