{
    "$schema" : "http://json-schema.org/draft-04/schema#",
    "title": "METHOD /endpoint",
    "description": "Schema for a not successful request to any endpoint",
    "type": "object",
    "properties": {
        "code": {
          "type": "integer",
          "description": "code error"
        },
        "message": {
          "type": "string",
          "description": "Error message"
        },
        "trace" : {
            "type": "string",
            "description": "Entire Stack Trace for the endpoint request error"
        },
        "link" :{
            "type": "string"
        }
    },
    "required": [
        "status",
        "deleted"
    ]
}
