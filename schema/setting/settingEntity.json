{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://api.veridu.com/1.0/schema/setting/settingEntity.json",
    "title": "Setting",
    "type": "object",
    "properties": {
        "company_id": {
            "description": "Company owner of the Credential",
            "type": "integer"
        },
        "section": {
            "description": "Section name",
            "type": "string",
            "pattern": "[a-zA-Z0-9_-]+"
        },
        "property": {
            "description": "Property name",
            "type": "string",
            "pattern": "[a-zA-Z0-9_-]+"
        },
        "value": {
            "description": "Property value",
            "type": "string",
            "pattern": ""
        },
        "created_at": {
            "description": "Setting's creation unixtimestamp",
            "type": "integer"
        },
        "updated_at": {
            "description": "Setting's last updated unixtime",
            "type": ["integer", "null"]
        }
    },
    "required": [
        "section",
        "property",
        "value",
        "created_at",
        "updated_at"
    ]
}
