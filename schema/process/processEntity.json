{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://api.veridu.com/1.0/schema/user/processEntity.json",
    "title": "Process",
    "type": "object",
    "properties": {
        "name": {
            "description": "Process Name",
            "type": "string",
            "pattern": ""
        },
        "event": {
            "description": "Event that triggered the process",
            "type": "string",
            "pattern": ""
        },
        "user_id": {
            "description": "User owner of the process",
            "type": "integer"
        },
        "created_at": {
            "description": "Process's creation unixtime",
            "type": "integer"
        },
        "updated_at": {
            "description": "Process's last update unixtime",
            "type": ["integer", "null"]
        }
    },
    "dependencies": {
    },
    "required": [
        "name",
        "event",
        "user_id",
        "created_at",
        "updated_at"
    ]
}
