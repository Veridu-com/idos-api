{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "GET /companies",
    "type": "object",
    "properties": {
        "status": {
            "description": "Boolean status",
            "type": "boolean"
        },
        "data": {
            "type": "array",
            "items": {
                "description": "Company Entity data",
                "$ref": "companyEntity.json",
                "minItems": 0,
                "uniqueItems": true
            }
        },
        "updated": {
            "description": "Unixtime of the last update",
            "type": "integer"
        }
    },
    "required": [
        "status",
        "data",
        "updated"
    ]
}
