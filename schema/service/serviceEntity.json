
{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://api.idos.io/1.0/schema/service/serviceEntity.json",
    "title": "Service",
    "type": "object",
    "properties": {
        "name": {
            "description": "Service Name",
            "type": "string",
            "pattern": ""
        },
        "access": {
            "description": "Access level of the service",
            "type": "integer"
        },
        "enabled": {
            "description": "If the service is enabled",
            "type": "boolean"
        },
        "listens": {
            "description": "Listens attribute",
            "type": "array",
            "minItems": 0,
            "uniqueItems": true
        },
        "triggers": {
            "description": "Triggers attribute",
            "type": "array",
            "minItems": 0,
            "uniqueItems": true
        },
        "created_at": {
            "description": "Company's creation unixtime",
            "type": "integer"
        },
        "updated_at": {
            "description": "Unixtime of the last update",
            "type": ["integer", "null"]
        }
    },
    "dependencies": {
        "slug": ["name"]
    },
    "required": [
        "name",
        "access",
        "enabled",
        "triggers",
        "triggers",
        "enabled",
        "created_at",
        "updated_at"
    ]
}
